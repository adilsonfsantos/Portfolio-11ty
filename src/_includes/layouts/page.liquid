---
layout: default
---
<article class="page grid--module" role="article">
  {%- comment -%}
    <div class="page__header">
    <div class="page__header--container"><h1 class="page__header--title" itemprop="name headline">{{ title | escape }}</h1></div>
  </div>
  {%- endcomment -%}
  <div class="page__content" itemprop="articleBody">
    <div class="page__content--container">
      {{ content }}
    </div>
  </div>
</article>
{% include contact %}
